{
    "name": "softspring/doctrine-simple-translation-type-bundle",
    "description": "This package provides a simple translation type for doctrine using JSON type.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "symfony/form": "^6.2",
        "symfony/http-kernel": "^6.2",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "9999999-dev",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\DoctrineSimpleTranslationTypeBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
