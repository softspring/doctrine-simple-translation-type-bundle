{
    "name": "softspring/doctrine-simple-translation-type-bundle",
    "description": "This package provides a simple translation type for doctrine using JSON type.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "symfony/form": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "twig/twig": "^3.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\DoctrineSimpleTranslationTypeBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev"
        }
    }
}
